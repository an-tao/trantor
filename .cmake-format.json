        "dangle_parens": true,
        "dangle_align": "prefix",
        "min_prefix_chars": 4,
        "max_prefix_chars": 10,
        "max_lines_hwrap": 2,
        "line_ending": "unix",
        "command_case": "canonical",
        "keyword_case": "unchanged",
        "always_wrap": [],
        "enable_sort": true,
        "autosort": false,
        "require_valid_layout": false,
        "layout_passes": {}
    }
