{
    "parse": {
        "additional_commands": {
            "foo": {
                "flags": [
                    "BAR",
                    "BAZ"
                ],
                "kwargs": {
                    "HEADERS": "*",
                    "SOURCES": "*",
                    "DEPENDS": "*"
                }
            }
        },
        "override_spec": {},
        "vartags": [],
        "proptags": []
    },
    "format": {
        "disable": false,
        "line_width": 100,
        "tab_size": 4,
        "use_tabchars": false,
        "fractional_tab_policy": "use-space",
        "max_subgroups_hwrap": 2,
        "max_pargs_hwrap": 6,
        "max_rows_cmdline": 2,
        "separate_ctrl_name_with_space": false,
